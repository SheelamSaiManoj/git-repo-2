<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <form action="/module" method="post">
        <button type="submit" name="btn" value="donor">donor module</button><br>
        <button type="submit" name="btn" value="receiver">receiver module</button><br>
    </form>
    <h1>Uploaded Images</h1>
	<div>
		<% items.forEach(function(image) { %>
		<div>
			<div>
				<img src="data:image/<%=image.img.contentType%>;base64,
					<%=image.img.data.toString('base64')%>">
				<div>
					<h5><%= image.id %></h5>
                    <p><%= image.name %></p>
					<p><%= image.quantity %></p>
					<p><%= image.expiry_date %></p>
				</div>
			</div>
            <button type="submit"></button>
		</div>
		<% }) %>
	</div>
</body>
</html>